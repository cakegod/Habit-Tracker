{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "types": ["@astrojs/image/client"],
    "isolatedModules": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"],
      "@util/*": ["src/util/*"]
    }
  }
}
